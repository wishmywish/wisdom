<script>
    //    菜单轮换点击事件
    $(".choose li").click(function () {
            var index = $(".choose li").index($(this));
            $(this).addClass("active").siblings().removeClass("active");
            $divList = $("#formList").children("div");
            $divList.hide();
            $divList.eq(index).show();
            var left=110*index;
            $("#line").css('margin-left',left);
        });


</script>

