/*主体设置*/
html,body{
    margin: 0 auto;
    font-size: 12px;
    font-family: Arial, Helvetica, sans-serif,微软雅黑,宋体;
    background: #fafafa;
    padding:0;
}
*{margin:0 auto; padding:0;}
ul{list-style: none;margin:0;padding:0;}
img{border:none;}
a{text-decoration:none;color: #95c8e8;outline:none;cursor: pointer;}
a:link{color:#95c8e8;text-decoration:none;outline:none;}
a:visited{text-decoration:none;color:#95c8e8;outline:none}
a:active{text-decoration:none;color:#95c8e8;outline:none}
a:hover{text-decoration:none;color:#409ad6;outline:none}

/*start===后台登录页===*/
.align-center{width:100%;height:420px;position:fixed;top:50%;margin-top:-210px;}/*垂直居中*/
.login_head{width:100%;height: 60px;}
.login_head .login_head_title{width: 660px;height: 60px;}

.login_body{width:100%;height: 310px;background-color:#376db7;}
.login_body .login_body_title{width: 710px;height: 310px;}
.login_body_img{width:302px;height:290px;float:left; margin-top: 20px;}
.login_body_from{width:304px;float:left;margin-left: 80px;margin-top: 30px;}
.login_body_from .login_top{width: 304px;height: 41px;}
.login_body_from .login_center{width: 304px;height: 170px;}

.login_body_from .login_down{width: 304px;height: 47px;}

.login_foot{width:100%;height: 50px;}
.login_foot .login_foot_title{width: 660px;height: 50px; line-height: 50px;}
.login_foot .login_foot_title .login_company_info{width:200px;height: 50px;margin-left:400px;text-align: center;color: #D0D0D0;}
/*end===后台登录页===*/


/*start===后台头部===*/
.page_top{width: 100%;height: 50px;line-height: 50px;background-color: #0086c9;line-height: 50px;color:#fff;}
.page_top .page_top_title{width: 1000px;height: 50px;}
.page_top .page_top_title .page_top_company{width: 630px;height: 50px;float: left;font-size: 20px; font-weight: bold;}
.page_top .page_top_title .page_top_menu{width: 270px;height: 50px;float: left;}
.page_top .page_top_title .page_top_info{width: 100px;height: 50px;float: left;}
/*end===后台头部===*/

/*start===后台左侧菜单===*/
.menu{width: 98px;border-right: 2px #eeeeee solid;float: left;}
.menu_button{width: 60px;}
.menu li{margin-top: 36px;cursor: pointer;}
/*end===后台左侧菜单===*/

/*start===后台首页==*/
.body_frame{width: 998px;border-right: 2px #eeeeee solid;}
.body_right{width: 898px; float: left;background-color: #fff;}
.body_right_home{width: 650px;float: left; height: 500px;border-right:1px solid #eeeeee;margin-top: 30px;float: left; }
.body_right_home p{line-height: 40px;margin-left: 30px;}
.body_right_button_list{width: 475px;margin-top: 40px;}
.body_right_button_list .body_right_button_jj{margin-right: 36px;margin-bottom: 17px;cursor: pointer;float: left;}
.body_right_button_wz{width: 90px;height: 20px;line-height: 20px; text-align: center; color: #fff;}

.body_right_right{width: 240px;margin-top: 30px;float: left;}
.right_statistics{width: 200px;}
.right_statistics .statistics_line{width: 100%;}
.right_statistics .statistics_line .statistics_wz_1{width: 100%;height: 25px;line-height: 25px;font-weight: bold;}
.right_statistics .statistics_line .statistics_wz_line{width: 100%;height: 15px;border: 1px #0086c9 solid;}
.right_statistics .statistics_line .statistics_wz_2{width: 100%;height: 20px;line-height: 20px;font-size: 9px;color:#D0D0D0;}

.company_info{width: 200px;background-color:#fbfbfb;margin-top: 35px;height: 200px;}
.company_info .company_info_on{width: 170px;height: 50px;text-align: center;line-height: 50px;}
.company_info .company_info_on_ico{width: 170px;height: 50px;text-align: center;line-height: 20px;}
/*end===后台首页===*/

/*start===后台组织架构==*/
.button_bg_80{width:80px;height: 25px;line-height: 25px;text-align: center;background-color: #409ad6;color:#fff;cursor: pointer;}
.button_bg_40{width:40px;height: 25px;line-height: 25px;text-align: center;background-color: #409ad6;color:#fff;cursor: pointer;}
.body_right_menu{width: 260px; float: left;margin-top: 15px;}
.body_right_list{width:636px;float:left;border-left: 2px solid #eeeeee;margin-top: 15px;}
.body_right_list_button{width:580px;height: 25px;}

.body_right_list_tab{width:100%;min-height:40px;margin-top:15px;border-top:1px solid #eeeeee;text-align: center;}
.body_right_list_tab li{float: left;width:25%; min-height:40px;line-height:35px;cursor: pointer;}
.body_right_list_tab li:hover{background-color:#409ad6;color:#fff}
.body_right_list_action{width: 580px;height: 35px; background-color: #eeeeee;}
.select_all{width:35px;height: 35px;float: left;}
.body_right_list_action_button{width:190px;height: 30px;float: left;padding-top: 5px;}
.body_right_list_action_search{width: 200px;height: 35px;float: right;margin-right: 20px;}

.body_right_list_user{width:580px;margin-top: 5px;}
.body_right_list_user li{float: left;text-align: center;height: 40px;line-height: 40px;color:#7d7d7d;}

.page{width:560px;height: 30px;line-height: 30px;text-align: right;margin-right: 28px;float: right;background-color: #eeeeee;padding-right: 20px;}

