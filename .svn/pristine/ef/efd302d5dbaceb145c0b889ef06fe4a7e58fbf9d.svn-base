			var toDate = new Date(oldToDate.getTime() - 7*24*60*60*1000);  //后一天
			document.getElementById("zhou_dateF").value = fromDate.Format("yyyy-MM-dd")
			document.getElementById("zhou_dateT").value = toDate.Format("yyyy-MM-dd")
