        var upfile3ReturnUrl = $("#upfile3ReturnUrl").val();
        $("#upfileForm3").ajaxSubmit({
            dataType:  "json", //数据格式为json 
            success: function(data) { //成功
                if(data.error_no == "999999") {
                    layer.msg("上传失败",{icon: 8});
                } else {
                    layer.msg("上传成功");
                    if(upfile3OldName != "") {
                        upfile3OldName += ",";
                        upfile3Type += ",";
                        upfile3Size += ",";
                        upfile3ReturnUrl += ",";
