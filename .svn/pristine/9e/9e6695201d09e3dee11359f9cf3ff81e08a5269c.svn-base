/*主体设置*/
html,body{
    margin: 0 auto;
    font-size: 12px;
    font-family: Arial, Helvetica, sans-serif,微软雅黑,宋体;
    background: #fafafa;
    padding:0;
    width:100%;

}
*{ margin: 0; padding: 0; list-style: none;}
* {margin: 0 auto; padding:0;}
ul{list-style: none;margin:0;padding:0;}
img{border:none;}
a{text-decoration:none;color: #95c8e8;outline:none;cursor: pointer;}
a:link{color:#95c8e8;text-decoration:none;outline:none;}
a:visited{text-decoration:none;color:#95c8e8;outline:none}
a:active{text-decoration:none;color:#95c8e8;outline:none}
a:hover{text-decoration:none;color:#409ad6;outline:none}
input{outline: none;}
/*头部样式*/
.header {
    background: #f3f4f9 none repeat scroll 0 0;
    border-bottom: 1px solid #d7d8db;
    height: 40px;
    color: #717071;

}
.head_left{
    float: left;
    margin: 12.5px 0 12.5px 360px;
}
.head_right{
    float: right;
    margin: 12.5px 365px 12.5px 0;
}
.head_right ul li{
    float: left;
    list-style:none;

}
.head_right ul span{
    margin: 0 12px;
}
.head_right a{color: #7d7d7d;}
/*内容*/
.body{
    background-color: #fff;
    height: 100%;
    width: 100%;
}
.k0{height: 35px;}/*顶部/底部留白*/
.content{
    background-color: #f3f4f9;
    border: 1px solid #eeeeee;
    height: 100%;
    min-height: 500px;
    overflow: hidden;
    width: 1200px;

}
.content_top{height: 75px;width: 100%;background: #fff;}
.logo{width: 190px;float: left;background: #f47f12;height: 75px;}
.menu_nav{width: 1010px;height: 75px;float: right}
.menu_nav_right{ margin-left:25px;font-size: 16px;}
.menu_nav_box{float: left;border-right: 1px #f3f4f9 solid;}
.menu_nav_box span{margin: 29px 0;display: inline-block}
.menu_nav_box span a{color: #7d7d7d;margin-left: 25px;}
.content_body{width: 100%;min-height: 600px;}
.content_menu{width: 190px;float: left;border-right: 1px #fff;background: #2a2d33;min-height: 600px;}
.menu_ul{
    width:180px;
    background: #2a2d33;
    font-size: 14px;
    margin:0 auto;
    position:relative;
    z-index:0;}
.menu_ul li{
    height:40px;
    line-height:40px;
    text-align:center;}
.menu_li a{
    color:#fff;
    display:block;}
.menu_li a:hover{
    color:#f47f12;
    text-decoration:none;}
.menu_li a.hover{
    color:#f47f12;
}
.ment{color: #b4b4b5;font-size: 12px;margin-top: 16px;}
.ment img{float: left;margin-left: 34px;}
.ment li{height: 33px;text-align: left;cursor: pointer;height: 39px;line-height: 2px;padding-bottom: 3px;padding-left: 69px;}

#lanPos{
    height:40px;
    line-height:40px;
    background: #2a2d33;
    border-left:3px solid #f47f12;
    position:absolute;
    left:-5px;
    top:0;
    z-index:-1;
    transition:top .2s;
}
.content_right{width: 1010px;}
/*底部*/
.footer{
    color: #7d7d7d;
    height: 30px;
    font-size: 11px;
}
.footer_left{
    float: left;
    margin-left: 357px;
}
.footer_left ul li{
    float: left;
    list-style:none;
}
.footer_right{
    float: right;
    margin-right: 360px;
}
.footer_right a{ color: #7d7d7d;}