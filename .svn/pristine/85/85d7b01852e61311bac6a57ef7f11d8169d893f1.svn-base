.table_top_left{margin-left:20px;float:left;margin-top:10px;color:#747474}
.table_top_right{margin-right:10px;float:right;height:25px;text-align:center;color:#fff;line-height:25px;margin-top:5px;}
.table_top_right ul li{background-color:#14BCE1;float:right;margin-right:20px;width:70px;}
.table1_center{width:100%;min-height:400px;background-color:#fff;}
.table1_center_top{margin-left:20px;float:left;text-align:center;margin-top:20px;}
.table1_center_top ul li{height:25px;line-height:25px;background-color:#fff;float:left;text-align:center;width:70px;}
.table1_list_li_1{border:1px #AFAFAF solid; border-right:0px;}
.task_list_li_2{border:1px #AFAFAF solid;}
.table1_center_body{width:100%;float:left;margin-top:20px;}
.table1_center_body_left{width:305px;min-height:320px;float:left;margin-left:20px;}
.table1_center_body_right{width:305px;min-height:320px;float:right;margin-right:60px;}
.body_title{background: #f4f3f9 none repeat scroll 0 0;border-radius: 12px;height: 25px;line-height: 25px;margin-top: 5px;text-align: center;width: 50px;}
.body_chart{width:100%;height:200px;margin-top:10px;}
.body_percent{width:80px;height:25px;line-height:25px;text-align: center;margin-top:20px;color:#fff;background-color:#4EB54A;}
.body_foot{background: #f4f3f9 none repeat scroll 0 0;border-radius: 12px;height: 25px;margin-top: 40px;width: 202px;}
.body_foot ul li{float:left;margin-left:20px;margin-top:2px;line-height: 25px;}
.table2_center_body_left{width:404px;min-height:320px;float:left;margin-left:20px;}
.table2_center_body_right{width:250px;height:250px;margin-right:60px;float:right;margin-top:40px;border:1px #DDDDDD solid;border-top:0px;border-right:0px;background-color:#DDDDDD;}
.table2_center_body_right ul li{height:124px;width:124px;float:left;position:relative;overflow:hidden;background:none repeat scroll 0 0 #fff;margin:1px 1px 0 0}
.table2_right_top{margin-top:20px;text-align: center;height:20px;}
.table2_right_foot{height:60px;margin-top:10px;font-size:20px;color:#F37569;text-align:center;line-height:60px}
.table3_center_body{width:100%;float:left;margin-top:20px;height:470px;}
.table3_center{width:100%;height:500px;background-color:#fff;}
.table3_center_body_left{width:550px;height:360px;float:left;margin-left:20px;}
.table3_center_body_right{width:250px;height:250px;margin-right:60px;float:right;margin-top:120px;}
/*底部*/
.footer{color: #7d7d7d;height: 30px;font-size: 11px;}
.footer_left{float: left;margin-left: 357px;}
.footer_left ul li{float: left;list-style:none;}
.footer_right{float: right;margin-right: 360px;}
.footer_right a{ color: #7d7d7d;}
/*review开始 content_right */
.content_right .cons_top{ height: 85px; border:1px solid #d2d3d5; border-left: none; border-right: none;line-height: 85px;}
.cons_top .cons_tab{ width: 500px;}
.cons_tab .current{color: #e57f13;}
.cons_tab span{ font-size: 14px; color: #717071; cursor: pointer; background: url("../images/nav_bg.png") no-repeat right center; padding: 0 20px; font-family: '微软雅黑';}
.cont_tab .nobg{ background: none;}
.cons_top .p1{width: 163px; color: #717071;}
.cons_top .p1 a{ color: #717071;}
.content_right .cont{ width: 100%;}
.cont .cur{display:none;}
.cont_in{ width: 100%; font-size: 14px; color: #717071; font-weight: 500;overflow: hidden;}
.screen{ width: 958px; padding: 35px 0 10px 25px; position: relative; }
.screen .sp2{ margin-left:60px;}
.screen button{ width: 70px; height: 25px; background: #e57f13; border-radius: 5px; border: none; color: #FFFFFF; position: absolute; right: 0px;}
.screen_con{ position: relative; width: 958px; margin-left: 25px; border: 1px solid #d2d3d5; overflow: hidden; background: #fff; padding-bottom: 40px;}
.screen_con .screen_con_top{ width: 100%;height: 40px; background: #f3f4f9; line-height: 40px; border-bottom: 1px solid#717071; }
.screen_con_top a{display: inline-block;  color: #717071;}
.screen_con ul{float: left; margin:15px 0 0 0;width: 100%;}
.screen_con ul li{ float: left;margin-left: 20px; margin-right: 26px;}
.screen_con .con_li{ color: #e57f13;}

.screen_con .pag{margin:20px 20px 0 0; float: right;position: absolute; right: 20px; bottom: 10px; }
.pag button{width: 14px; height: 14px; text-align: center; line-height: 14px; display: inline-block; cursor: pointer;}
/*已中标*/
.screen_con .common{width: 958px; overflow: hidden;}
.screen_con .screen_con1{width: 478px; height: 148px;  border-right: 1px solid #f9f9f9; position: relative;}

.screen_con1 p{margin:10px 0 0 32px; color: #8b8b8b; font-size: 14px;}
.screen_con1 p:first-child{ margin-top: 10px}
.screen_con .screen_con1 button{ position: absolute; right: 20px; top: 20px; background: #e68012; color: #fff;width: 90px; height: 25px; border-radius: 5px;}
.common dl{ width: 478px; height: 123px; border-right: 1px solid #f9f9f9; background:#f3f4f9; }
.common dl dt{ margin-left:32px; margin-top: 20px; float: left;}
.common dl dd{  margin-left:32px;margin-top: 10px; display: inline-block; float: left;}

