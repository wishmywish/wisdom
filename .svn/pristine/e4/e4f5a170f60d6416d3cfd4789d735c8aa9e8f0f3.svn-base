                $("#form6 .userbuss").append('<span>&nbsp;&nbsp;&nbsp;&nbsp;审批人员</span><div id="users6" style="width: 580px;float: left;margin: -18px 0 0 72px;"><div class="pers1" onclick="selectUser(\'\',1,\'users6\');"><i style="color: #fff;padding: 10px;cursor: pointer;" class="fa fa-plus fa-3x"></i></div></div>');
