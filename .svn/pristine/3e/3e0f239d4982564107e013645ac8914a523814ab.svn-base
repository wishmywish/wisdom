/*主体设置*/
html,body{
    margin: 0 auto;
    font-size: 12px;
    font-family: Arial, Helvetica, sans-serif,微软雅黑,宋体;
    background: #fafafa;
    padding:0;
    width:100%;
}
*{ margin: 0; padding: 0; list-style: none;}
* {margin: 0 auto; padding:0;}
button{ border: none;}
ul{list-style: none;margin:0;padding:0;}
img{border:none;}
.fl{ float: left;}
.rg{ float: right;}
a{text-decoration:none;color: #95c8e8;outline:none;cursor: pointer;}
a:link{color:#95c8e8;text-decoration:none;outline:none;}
a:visited{text-decoration:none;color:#95c8e8;outline:none}
a:active{text-decoration:none;color:#95c8e8;outline:none}
