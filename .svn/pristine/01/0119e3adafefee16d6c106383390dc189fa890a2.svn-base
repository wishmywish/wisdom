/*head_top*/
.ui-content{padding: 0;}
.head_top{min-height: 30px; background: #f6f6f6;line-height: 30px;}
.head_top p{ color: #000; width: 92%; text-shadow: none; margin-left: 1.3em; word-wrap:break-word;}
.head_nav{ height: 30px; background: #fff; line-height: 33px;border-bottom:1px solid #ccc;  }
.head_nav .sp5{ color: #ff6600; }
.head_nav p{  width: 50%; }
.ui-page-theme-a .ui-btns,.ui-btn.ui-btn-a:visited{ background: none; color: #ccc;}
.ui-btn-icon-left:after, .ui-btn-icon-right:after, .ui-btn-icon-top:after, .ui-btn-icon-bottom:after, .ui-btn-icon-notext:after{ width: 14px; height: 14px;}
[class*="ui-block-"] > .ui-btn, [class*="ui-block-"] > .ui-select, [class*="ui-block-"] > .ui-checkbox, [class*="ui-block-"] > .ui-radio, [class*="ui-block-"] > button.ui-btn-inline, [class*="ui-block-"] > button.ui-btn-icon-notext, .ui-header [class*="ui-block-"] > button.ui-btn, .ui-footer [class*="ui-block-"] > button.ui-btn{ margin-left: 1.413em; margin-right: .823em;}
.ui-btns{padding: 0; margin: 0 4%; width: 14%; font-size: 12px; font-weight: normal; color: #ccc;}
.ui-btn-icon-left:after{left: 0; font-size: 12px; }
.ui-btn-icon-notext:after, .ui-btn-icon-left:after, .ui-btn-icon-right:after{ top: 74%;margin-left:3%;}
.ui-block-a .a4{ float: left; margin-top:0.5em; line-height: 1.5em; height: 16px; background: #fff; border: none; box-shadow:none;}
.nav_cent{ text-align: center;}
.ui-block-c .a5{ margin-left: 4%; margin-top:0.5em; line-height: 1.5em; float: right; height: 16px;background: none; border: none; box-shadow:none;}
.head_con{ border-bottom: 1px solid #ccc; background: #fff; line-height: 30px;min-height: 30px;}
.head_con p{ margin-left: 1.3em; word-wrap: break-word;}
.ui-shadow{ box-shadow: none;}
.head_con2{ width: 100%; overflow: hidden; margin-top: 1em; }
.head_con2 p{  margin-left: 1.3em;  overflow: hidden; position: relative; min-height: 30px; }
.head_con2 .inps{ position: static; left: 0 ; top: 0; margin:0;width: 12px; height: 12px; float: left;}
.head_con2 .ui-radio{ width: auto; height: auto; float: left; position: absolute; top: 2px; left: 0; margin: 0;}
.head_con2 p span{ float: left; }
.head_con2 .sp6{word-wrap: break-word; margin-left: 1.8em; vertical-align:middle;display:table-cell;   line-height: normal; position: relative;}
/*head_con1*/
.head_con1{ width: 100%; overflow: hidden;}
.head_con1 p{ margin:1em 1.3em 0 1.3em;  color: #7f7f80; }
.head_con1 .sp7{ margin-left: 1em;}
.head_con3{ margin-left: 1.3em; margin-right: 1.3em; margin-top: 1em; width: 92%;}
.ui_block{ padding: 2px; border: 1px solid #ccc; margin-right: 1.2em;}
.ui_block:last-child{ margin: 0;}
.head_con3 .ui-block{ width: 30%;!important; position: relative;}
.head_con3 .ui-block .sp8{ position: absolute; top: .5em; right: .5em; color: #ff6600;z-index: 10;}
.head_con2 .ui-checkbox{ position: absolute;top: 2px;left:0; width: 5%;  margin: 0;}

.heads{ background: #ff6600; width: 100%; height: 49px;}
.heads h1{ text-align: center; line-height: 49px; color: #fff; text-shadow: none; font-size: 2em;}
.head_con4{ width: 100%;}
.head_con4 button{ margin-left: 1.3em; width: 90%; color: #ff6600;}